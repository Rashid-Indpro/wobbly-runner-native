{
  "expo": {
    "name": "Wobbly Runner",
    "slug": "wobbly-runner-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wobblyrunner.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#09090B"
      },
      "package": "com.wobblyrunner.app"
    },
    "web": {
      "bundler": "metro"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/wobbly-runner-native"
    },
    "runtimeVersion": "1.0.0",
    "react-native-google-mobile-ads": {
      "android_app_id": "ca-app-pub-9218417844276973~5801993490"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ]
    ]
  }
}
